          var match = $( this ).attr('src').match(regExp);
          //console.log("embed");
          //console.log($( this ).attr('src'));
          //console.log(match);
          if(match != null ){
            $( this ).width( "100%" );
            $( this ).height( "auto" );
          }
      });
    }
    
     $('ul.nav li.dropdown').hover(function() {
		  $(this).find('.dropdown-menu').stop(true, true).fadeIn();
		}, function() {
		  $(this).find('.dropdown-menu').stop(true, true).fadeOut();
		});
   });
</script>


