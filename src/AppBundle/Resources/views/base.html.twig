    <![endif]-->

    <!-- Custom styles for this template -->


</head>
<!-- NAVBAR
================================================== -->
<body>
<section class="header">
	<div class="container">
		<nav class="navbar navbar-default" role="navigation">
  <!-- Brand and toggle get grouped for better mobile display -->
  <div class="navbar-header">
    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex2-collapse">
      <span class="sr-only">Toggle navigation</span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </button>
    <a class="navbar-brand" href="{{ path('homepage') }}"><img src="{{ asset('bundles/app/images/logo.jpg') }}" alt="" /></a>
  </div>

  <!-- Collect the nav links, forms, and other content for toggling -->
  <div class="collapse navbar-collapse navbar-ex2-collapse">
    <ul class="nav navbar-nav navbar-right">
    <li {% if app.request.attributes.get('_route') == 'homepage' %} class="select" {% endif %}><a href="{{ path('homepage') }}">Home</a></li>
   	<li {% if app.request.attributes.get('_route') == 'about-us' %} class="select" {% endif %}><a href="{{ path('about-us') }}">About  Us</a></li>
   	<li {% if app.request.attributes.get('_route') == 'services' %} class="select" {% endif %}><a href="{{ path('services') }}">Services</a></li>
   	<li {% if app.request.attributes.get('_route') == 'reviews' %} class="select" {% endif %}><a href="{{ path('reviews') }}">Reviews</a></li>
   	<li {% if app.request.attributes.get('_route') == 'gallery' %} class="select" {% endif %}><a href="{{ path('gallery') }}">Gallery</a></li>
   	<li {% if app.request.attributes.get('_route') == 'contact' %} class="select" {% endif %}><a href="{{ path('contact') }}">Contact</a></li>
    </ul>
  </div><!-- /.navbar-collapse -->

</nav>
	</div>
</section>

{% block body %}{% endblock %}

<section class="footer">
	<div class="container">
		<div class="row">
			<div class="col-sm-4">
				<div class="footerbox">
					<h2>Our Company</h2>
					<ul>
						<li><a href="{{ path('about-us') }}">About us</a></li>
						<li><a href="{{ path('services') }}">Services</a></li>
						<li><a href="{{ path('gallery') }}">Gallery</a></li>
						<li><a href="{{ path('reviews') }}">Reviews</a></li>
						<li><a href="{{ path('contact') }}">Contact us</a></li>
					</ul>
				</div>
			</div>
			<div class="col-sm-4">
        {% if website.hasSocial == true %}
				<div class="footerbox">
					<h2>Connect Socially</h2>
					<ul>
            {% if website.facebookLink is not null %}
						<li><a href="{{ website.facebookLink }}" target="_BLANK"><img src="{{ asset('bundles/app/images/global/social/ffb1.png') }}" alt="Connect On Facebook" />Facebook</a></li>
						{% endif %}
						{% if website.twitterLink is not null %}
						<li><a href="{{ website.twitterLink }}" target="_BLANK"><img src="{{ asset('bundles/app/images/global/social/ffb2.png') }}" alt="Connect On Twitter" />Twitter</a></li>
						{% endif %}
						{% if website.youtubeLink is not null %}
						<li><a href="{{ website.youtubeLink }}" target="_BLANK"><img src="{{ asset('bundles/app/images/global/social/ffb3.png') }}" alt="Connect On Youtube" />Youtube</a></li>
						{% endif %}
						{% if website.googleLink is not null %}
						<li><a href="{{ website.googleLink }}" target="_BLANK"><img src="{{ asset('bundles/app/images/global/social/ffb4.png') }}" alt="Connect On Google Plus" />Google +</a></li>
						{% endif %}
					</ul>
				</div>
        {% endif %}
			</div>
			<div class="col-sm-4">
				<div class="footerbox wty">
					<h2>Recent Jobs</h2>
					<ul>
            {% for item in footer_images %}
            	<li><img src="{{ asset('images/gallery/') }}{{ item.path }}" alt="{{ item.title }}" /></li>
            {% endfor %}
					</ul>
				</div>
			</div>
		<!--	<div class="col-sm-3">
				<div class="footerbox">
					<div class="footerlogo">
						<a href="{{ path('homepage') }}"><img src="{{ asset('bundles/app/images/footerlogo.jpg') }}" alt="" /></a>
					</div>
				</div>
			</div>
    -->
		</div>
	</div>
</section>
<section class="copyright">
	<div class="container">
		<div class="row">
			<div class="col-sm-12">
				<div class="cpy">{{ website.copyright }}</div>
			</div>
		</div>
	</div>
</section>
