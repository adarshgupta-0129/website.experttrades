$( document ).ready(function() {
    var is_mobile = false;

    if( $('.captop').css('display')=='none') {
        is_mobile = true;
    }

    // change iframes
    if (is_mobile == true) {

      $('iframe').each(function() {
        var regExp = /.*(youtube|vimeo).*/;
          var match = $( this ).attr('src').match(regExp);
          //console.log("embed");
          //console.log($( this ).attr('src'));
          //console.log(match);
          if(match != null ){
            $( this ).width( "100%" );
            $( this ).height( "auto" );
          }
      });
    }
 });
